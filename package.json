{
  "name": "@timkpaine/jupyterlab_miami_nights",
  "version": "0.3.0",
  "description": "A glowing 80s theme based on Synthwave '84 and  JupyterLab's Neon Night theme by yeebc",
  "author": "Tim P <t.paine154@gmail.com>",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyterlab-theme",
    "neon",
    "cyberpunk"
  ],
  "license": "BSD-3-Clause",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "lib/*.css",
    "style/*"
  ],
  "sideEffects": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "lib/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "highlight.js": "^10.4.1"
  },
  "devDependencies": {
    "base64-inline-loader": "^1.1.1",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.3",
    "rimraf": "~3.0.0",
    "typedoc": "^0.15.4",
    "typescript": "~3.7.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "resolutions": {
    "highlight.js": "^10.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "lib/index.css"
  },
  "homepage": "https://github.com/timkpaine/jupyterlab_miami_nights",
  "bugs": {
    "url": "https://github.com/timkpaine/jupyterlab_miami_nights/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timkpaine/jupyterlab_miami_nights.git"
  }
}
